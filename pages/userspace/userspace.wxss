}

.gender_womanView {
  height: 13px;
  color: white;
  float: left;
  width: 13px;
  background-color: #fda3e6;
  border-radius: 8px;
  margin-top: 2px;
  margin-right: 8px;
}

.tagAreaView {
  margin-top: 8px;
  clear: both;
}

.signatureView {
  clear: both;
  font-size: 12px;
  padding-top: 5px;
  padding-bottom: 5px;
}

.hideTextView {
  font-size: 10px;
  float: left;
  background-color: #eff2fb;
  color: #8e8e8e;
  padding-left: 5px;
  padding-right: 5px;
  padding-bottom: 1px;
  padding-top: 1px;
  border-radius: 5px;
  margin-right: 5px;
}

.chatTextView {
  float: left;
  padding-top: 4px;
  padding-left: 3px;
  font-size: 12px;
  color: #8e8e8e;
}
