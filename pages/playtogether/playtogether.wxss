.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 14rpx;
  color: #000;
  padding-left: 10px;
  padding-right: 3px;
  overflow: hidden;
  width: 100%;
  height: 100%;
}

page {
  background-color: #eff2fb;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  position: fixed;
}

swiper {
  height: 100%;
  width: 100%;
  display: block;
}

swiper-item {
  display: block;
  overflow: hidden;
  will-change: 'transform';
  height: 100%;
  width: 100%;
}

.topSpinner {
  width: 100%;
  text-align: center;
  font-size: 14px;
  padding-top: 15px;
}

.spinner {
  margin: 100px auto 0;
  width: 150px;
  text-align: center;
}

.spinner>div {
  width: 8px;
  height: 8px;
  background-color: #09f;
  border-radius: 100%;
  display: inline-block;
  -webkit-animation: bouncedelay 1.4s infinite ease-in-out;
  animation: bouncedelay 1.4s infinite ease-in-out;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  margin-left: 5px;
}

.spinner .bounce1 {
  -webkit-animation-delay: -0.32s;
  animation-delay: -0.32s;
}

.spinner .bounce2 {
  -webkit-animation-delay: -0.16s;
  animation-delay: -0.16s;
}

