.momentContainer {
  width: 100%;
}

.collectView {
  float: left;
  font-size: 20px;
  padding-left: 20px;
  padding-top: 7px;
  color: #8e8e8e;
  flex-grow: 1;
}

.itemTitleView {
  float: left;
  font-size: 17px;
  padding-left: 20px;
