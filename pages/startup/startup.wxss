page {
  transition: all 0.3s linear 0s;
  margin-top: 0px;
  width: 100%;
  height: 100%;
}

.startImgView {
  width: 100%;
  height: 100%;
}

.imgView {
  width: 100%;
  height: 100%;
}

.checkoutView {
  width: 100%;
  height: 100%;
  position: fixed;
}

.header {
  margin: 90rpx 0 90rpx 50rpx;
  text-align: center;
  width: 650rpx;
  height: 300rpx;
  line-height: 450rpx;
}

.content {
  margin-left: 50rpx;
  margin-bottom: 90rpx;
  text-align: center;
  font-size: 20px;
}

.content view {
  display: block;
  color: #9d9d9d;
  margin-top: 40rpx;
}

.bottom {
  border-radius: 50rpx;
  margin: 70rpx 50rpx;
  font-size: 35rpx;
}

.backView {
  background: url(https://www.pikaer.com/common/image/20190713_115085_22943ca3-4fed-4615-914f-899bff1bd433.jpg) no-repeat;
  position: fixed;
  background-attachment: fixed;
  background-size: cover;
  height: 100%;
  width: 100%;
  opacity: 0.2;
}

.loginView {
  height: 100%;
  width: 100%;
  position: absolute;
}
