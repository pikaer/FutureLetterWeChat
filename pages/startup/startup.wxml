<view wx:if="{{showStartUp}}" class="checkoutView">
  <view class="startImgView">
    <image class="imgView" src="../../content/images/start.jpg"></image>
  </view>
</view>

<view wx:else>
  <view class="backView"></view>
  <view class="loginView">
    <view class='header'>
      <view class="iconfont face" style="color:#0099ff;font-size:90px;"></view>
    </view>
    <view class='content'>
      <view>登录后发现更多精彩~</view>
    </view>
    <button class='bottom' type='primary' open-type="getUserInfo" lang="zh_CN" bindgetuserinfo="bindGetUserInfo">
      微信快捷登录
    </button>
  </view>
</view>